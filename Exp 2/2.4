#include<iostream>
#include<cmath>
using namespace std;

int main() 
{   
    int n;
    cout<<"Enter the Number:";
    cin>>n;
    double sumlog=0;
    for(int i=1;i<=n;i++)
    {
        sumlog=sumlog+log10(i);
    }
    int d=sumlog+1;
    
    cout << "Number of digits in the factorial are :"<<d;
    

    return 0;
}
