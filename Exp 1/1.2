#include <iostream>
#include <vector>
using namespace std;

int main() {
    int r, c;
    cout << "Enter rows and columns: ";
    cin >> r >> c;

    vector<vector<int>> a(r, vector<int>(c));

    cout << "Enter matrix:\n";
    for(int i=0;i<r;i++)
        for(int j=0;j<c;j++)
            cin >> a[i][j];

    cout << "Transpose:\n";
    for(int j=0;j<c;j++) {
        for(int i=0;i<r;i++)
            cout << a[i][j] << " ";
        cout << endl;
    }
    return 0;
}
